@model Lanche
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@


    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="@Model.ImagemUrl" class="img-rounded" width="200" height="200" />
        </div>
        <div class="caption">
            <h3 class="pull-right">Lanche: @Model.Preco.ToString("c")</h3>
            <h3>@Model.Nome</h3>
            <p>Preço: @Model.DescricaoCurta</p>
        </div>

    </div>



